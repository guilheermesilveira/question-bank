{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "server={localhost};port={port};user={user};password={password};database={name_db}"
  },
  "JwtSettings": {
    "HoursUntilExpiry": 2,
    "KeyPath": "enter a path on your computer to store the secret keys"
  },
  "AllowedHosts": "*"
}
